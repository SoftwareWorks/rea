func isPrime(n: Int): Bool {
    var i = 2
    while (i * i <= n) {
        if (n % i == 0) {
            return false
        }
        var i = i + 1
    }
    return true
}

func main() {
    // print the first 10 primes greater than one million
    var n = 0
    var i = 1000000
    while (n < 10) {
        if (isPrime(i)) {
            print(i)
            var n = n + 1
        }
        var i = i + 1
    }
}
